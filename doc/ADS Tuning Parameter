    "from keras.optimizers import Adamax\n",
    "def create_model(learn_rate):\n",
    "    model = Model(inputs=inp, outputs=x)\n",
    "    optimizer = Adamax(lr=learn_rate)\n",
    "    model.compile(loss='binary_crossentropy',\n",
    "                  optimizer=optimizer,\n",
    "                  metrics=['accuracy'])\n",
    "    return model"
   ]
  },
